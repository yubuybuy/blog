{
  "rewrites": [
    {
      "source": "/boss-admin/(.*)",
      "destination": "/boss-admin"
    },
    {
      "source": "/structure/(.*)",
      "destination": "/boss-admin"
    },
    {
      "source": "/desk/(.*)",
